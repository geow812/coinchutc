                android:name="android.support.PARENT_ACTIVITY"
                android:value="utc.coinchutc.MainActivity" />
        </activity>
        <activity
            android:name="utc.coinchutc.ReglesDuJeuActivity"
            android:label="@string/title_activity_regles_du_jeu"
            android:parentActivityName="utc.coinchutc.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="utc.coinchutc.MainActivity" />
        </activity>
        <activity
            android:name="utc.coinchutc.ConnexionActivity"
            android:label="@string/title_activity_connexion" >
        </activity>
        <activity
            android:name="utc.coinchutc.PartieActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_partie"
            android:parentActivityName="utc.coinchutc.CreerPartieActivity"
            android:theme="@style/FullscreenTheme" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="utc.coinchutc.MainActivity" />
        </activity>
        <activity
            android:name="utc.coinchutc.ReglePrincipeActivity"
            android:label="@string/principe"
            android:parentActivityName="utc.coinchutc.ReglesDuJeuActivity" >
