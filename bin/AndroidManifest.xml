<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="utc.coinchutc"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />

    <application
        android:name="utc.coinchutc.CoinchutcApplication"
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo" >
        <service android:name="jade.android.MicroRuntimeService" />

        <activity
            android:name="utc.coinchutc.MainActivity"
            android:label="@string/app_name"
            android:noHistory="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="utc.coinchutc.CreditsActivity"
            android:label="@string/title_activity_credits"
            android:parentActivityName="utc.coinchutc.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="utc.coinchutc.MainActivity" />
        </activity>
        <activity
            android:name="utc.coinchutc.RejoindrePartieActivity"
            android:label="@string/title_activity_rejoindre_partie"
            android:parentActivityName="utc.coinchutc.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="utc.coinchutc.MainActivity" />
        </activity>
        <activity
            android:name="utc.coinchutc.JoueursConnectesActivity"
            android:label="@string/title_activity_joueurs_connectes"
            android:parentActivityName="utc.coinchutc.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="utc.coinchutc.MainActivity" />
